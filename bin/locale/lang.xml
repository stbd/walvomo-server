	<!-- templates -->
	<message id="Wt.Auth.template.login">
<h4 class="left main-header-login-field-length">${tr:Wt.Auth.login-form-title}:</h4>

<div class="left">
	<div class="left Wt-auth-login">
		${<if:passwords>}
			<div>
				<label class="main-header-login-field-length" for="${id:user-name}">${user-name-label}</label>
				${user-name}
			</div>
			<div>
				<label class="main-header-login-field-length" for="${id:password}">${tr:Wt.Auth.password}</label>
				${password}
			</div>
			${<if:remember-me>}
				<div>
					<label for="${id:remember-me}">${remember-me-info}</label>
					${remember-me}
				</div>
			${</if:remember-me>}
		${</if:passwords>}
	</div>
	
	<div class="left">
		${login}
	</div>

	<div class="main-horizontalRule" runat="server"></div>

	${<if:oauth>}
		<div class="Wt-col-right">
		<h3 class="left main-header-logInViaOther">Or use:</h3>
		${icons}
		</div>
	${</if:oauth>}
	
	<div class="main-header-login-field block center-text">
		${lost-password} ${sep} ${register}
	</div>
</div>
	</message>

	<message id="Wt.Auth.template.registration">
    <div class="Wt-form Wt-auth-registration">
      <h2>${tr:Wt.Auth.registration-form-title}</h2>
      <p>${tr:Wt.Auth.registration-form-info}</p>

      ${password-description}

      <div class="Wt-fields">
        ${<if:user-name>}
	  <label for="${id:user-name}">${user-name-label}
	    ${user-name-info class="Wt-info"}
	    ${confirm-is-you class="Wt-info"}
	  </label>
	  ${user-name}
	${</if:user-name>}

        ${<if:choose-password>}
	  <label for="${id:choose-password}">${tr:Wt.Auth.choose-password}
	    ${choose-password-info class="Wt-info"}
	  </label>
	  ${choose-password}

	  <label for="${id:repeat-password}">${tr:Wt.Auth.repeat-password}
	    ${repeat-password-info class="Wt-info"}
	  </label>
	  ${repeat-password}
	${</if:choose-password>}

        ${<if:email>}
	  <label for="${id:email}">${tr:Wt.Auth.email}
	    ${email-info class="Wt-info"}
	  </label>
	  ${email}
        ${</if:email>}
        
	  <label for="${id:choose-gender}">${tr:Wt.Auth.gender}
	    ${choose-gender-info class="Wt-info"}
	  </label>
	  ${choose-gender}

	  <label for="${id:year-of-birth}">${tr:Wt.Auth.year-of-birth}
	    ${year-of-birth-info class="Wt-info"}
	  </label>
	  ${year-of-birth}
	  
	  <label for="${id:disclaimer}">${tr:Wt.Auth.disclaimer}
	    ${disclaimer-info class="Wt-info"}
	  </label>
	  ${disclaimer}	  
        
      </div>

      ${<if:oauth>}
        <br clear="both" />
	${oauth-description}
	<label>${tr:Wt.Auth.oauth}
	  <span class="Wt-info">${tr:Wt.Auth.oauth-info}</span>
	</label>
	${icons}
      ${</if:oauth>}

      <div class="Wt-buttons">
	${ok-button} ${cancel-button}
      </div>
    </div>
	</message>

	<message id="Wt.Auth.template.update-password">
    <div class="Wt-form Wt-auth-update-password">
      <h2>${tr:Wt.Auth.update-password-form-title}</h2>
      <p>${tr:Wt.Auth.update-password-form-info}</p>

      <div class="Wt-fields">
	<label for="${id:user-name}">${tr:Wt.Auth.user-name}
	</label>
	${user-name}

	${<if:old-password>}
	  <label for="${id:old-password}">${tr:Wt.Auth.old-password}
	    ${old-password-info class="Wt-info"}
	  </label>
	  ${old-password}
	${</if:old-password>}

        <label for="${id:choose-password}">${tr:Wt.Auth.new-password}
	  ${choose-password-info class="Wt-info"}
        </label>
        ${choose-password}

        <label for="${id:repeat-password}">${tr:Wt.Auth.repeat-new-password}
	  ${repeat-password-info class="Wt-info"}
        </label>
        ${repeat-password}
        
      </div>

      <div class="Wt-buttons">
	${ok-button} ${cancel-button}
      </div>
    </div>
	</message>

